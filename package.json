{
  "name": "micro-echo",
  "description": "echo microservice",
  "version": "1.1.1",
  "license": "MIT",
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.8.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "echo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zrrrzzt/micro-echo.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/micro-echo/issues"
  },
  "homepage": "https://github.com/zrrrzzt/micro-echo",
  "now": {
    "alias": [
      "echo.mikrotjeneste.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "axios": "0.16.1",
    "coveralls": "2.13.0",
    "nsp": "2.6.3",
    "nyc": "10.2.0",
    "standard": "10.0.2",
    "test-listen": "1.0.2"
  },
  "dependencies": {
    "marked": "0.3.6",
    "micro": "7.3.2"
  }
}
