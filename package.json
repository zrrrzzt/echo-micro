{
  "name": "micro-echo",
  "description": "echo microservice",
  "version": "1.2.1",
  "license": "MIT",
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.9.1"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "echo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zrrrzzt/micro-echo.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/micro-echo/issues"
  },
  "homepage": "https://github.com/zrrrzzt/micro-echo#readme",
  "now": {
    "alias": [
      "ekko.mikrotjeneste.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "axios": "0.18.0",
    "coveralls": "3.0.0",
    "micro-dev": "2.2.0",
    "nsp": "3.2.1",
    "nyc": "11.4.1",
    "standard": "11.0.0",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "markdown-it": "8.4.1",
    "micro": "9.0.2"
  }
}
